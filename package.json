  "contributors": [],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "start": "react-native start",
    "test": "jest --coverage",
    "link": "eslint",
    "postinstall": "rn-nodeify --install crypto,randombytes,process,stream,events,http,https,vm,url --hack",
