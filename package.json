    "vm-browserify": "1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2s",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/redux-mock-store": "^1.0.2",
    "babel-jest": "25.1.0",
    "eslint": "^6.5.1",
    "eslint-config-universe": "^2.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fetch-mock-jest": "^1.1.0-beta.3",
